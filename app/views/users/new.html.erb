<div class="sign-form">

<form class="new-user" action="<%= users_url %>" enctype="multipart/form-data" method="POST">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

    <div class="sign-input">
  <label for="user_email">Email</label>
  <input
    type="text"
    name="user[email]"
    id="user_email"
    value="<%= @user.email %>">
  </div>
  
  <div class="sign-input">
  <label for="user_name">Name</label>
  <input
    type="text"
    name="user[name]"
    id="user_name"
    value="<%= @user.name %>">
  </div>

  <div class="sign-input">
  <label for="user_password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="user_password">
  </div>
    
    <div class="sign-input">
    <input type="file" id="photo_image" name="user[avatar]">
  </div>
    <button>Sign Up</button>

</form>
</div>